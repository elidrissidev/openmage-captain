{
    "name": "elidrissidev/openmage-captain",
    "description": "Docker configuration generator for OpenMage development environments.",
    "keywords": ["openmage", "magento", "docker"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "bin": ["bin/openmage-captain"],
    "autoload": {
        "psr-4": {
            "OpenMage\\Captain\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.4"
        }
    }
}
