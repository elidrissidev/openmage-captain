{
    "name": "elidrissidev/openmage-docker",
    "description": "Docker configuration generator for OpenMage development environments.",
    "keywords": ["openmage", "magento", "docker"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "symfony/console": "^6.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "OpenMage\\Docker\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
